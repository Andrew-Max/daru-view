<thead>
  <tr>
    <th></th>
    <% column_number = 0 %>
    <% if table.data.sample.is_a?(Array)%>
      <% columns = table.data.sample.length %>
    <% else %>
      <% columns = 2 %>
    <% end %>
    <% (columns - 1).times do %>
      <th>Column: <%=column_number%></th>
      <% column_number += 1 %>
    <% end %>
  </tr>
</thead>
